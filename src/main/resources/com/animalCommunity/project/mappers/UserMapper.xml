        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.animalCommunity.project.mappers.UserMapper">
    <insert id="addUser">
        insert into users_tb(user_id, user_pw, nickname) values (#{userId},#{userPw},#{nickname})
    </insert>
</mapper>